{
  "manifest_version": 3,
  "name": "Custom Homepage Changer",
  "version": "1.2",
  "description": "Set a custom new tab page that doesn't display the URL.",
  "permissions": [
      "storage",
      "tabs"
  ],
  "chrome_url_overrides": {
      "newtab": "newtab.html"  
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/icon16.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
      }
  }
}
